{
  "name": "@mongodb-js/charts-embed-dom",
  "version": "3.3.1",
  "description": "JavaScript library for embedding MongoDB Charts",
  "keywords": [
    "mongodb",
    "charts",
    "chart",
    "data",
    "visualization",
    "dataviz",
    "dashboard",
    "embed",
    "graphs",
    "graph"
  ],
  "license": "Apache-2.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "unpkg": "dist/charts-embed-dom.umd.min.js",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "docs": "typedoc src/index.ts",
    "docs:check": "ts-node ./bin/lint-src-for-docs.ts",
    "start": "getstorybook dev -p 3002",
    "test": "jest",
    "test:ci": "yarn test --ci --runInBand --silent --detectOpenHandles",
    "compile:ts": "tsc --outDir build",
    "build:umd": "webpack --mode production",
    "build:ts": "tsc --project tsconfig.build.json",
    "build:cjs": "yarn build:ts --outDir dist/cjs",
    "build:esm": "yarn build:ts --module esnext --outDir dist/esm",
    "build:types": "yarn build:ts --stripInternal --declaration --emitDeclarationOnly --declarationDir dist/types && node ./bin/generate-types.js",
    "build": "yarn build:umd && yarn build:cjs && yarn build:esm && yarn build:types",
    "build:release": "./bin/pre-release.sh && yarn build",
    "release:npm": "yarn docs && ./bin/publish-to-npm-if-version-changed"
  },
  "homepage": "https://www.mongodb.com/products/charts",
  "repository": "https://github.com/mongodb-js/charts-embed-sdk",
  "devDependencies": {
    "@leafygreen-ui/palette": "^3.4.4",
    "@storybook/addon-controls": "^7.4.2",
    "@storybook/cli": "^7.4.2",
    "@storybook/html-webpack5": "^7.4.2",
    "@storybook/manager-api": "^7.4.2",
    "@storybook/theming": "^7.4.2",
    "@types/jest": "^29.2.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.157",
    "@types/sinon": "^7.5.1",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "csstype": "^2.6.21",
    "jest": "^29.6.2",
    "jest-chain": "^1.1.6",
    "jest-extended": "^4.0.1",
    "jsdom": "^17.0.0",
    "jsdom-global": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typedoc": "^0.17.0-3",
    "typescript": "^5.4.2",
    "webpack": "^5.88.2",
    "zod-2-ts": "^1.2.0"
  },
  "dependencies": {
    "@looker/chatty": "^2.2.0",
    "bson": "^5.4.0",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.19"
  },
  "alias": {
    "bson": "../../bson/lib/bson.cjs"
  }
}
